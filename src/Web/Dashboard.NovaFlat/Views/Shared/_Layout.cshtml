@model IEnumerable<Opciones>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Expires" content="-1">

    <title>@ViewBag.Title </title>
    <link rel="icon" type="image/png" sizes="16x16" href="~/Imagen/SIDE-Logo.png">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <link href="~/css/Loader.css" rel="stylesheet" />
    <link href="~/plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />


    <!-- Ionicons -->
    @*<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">*@


    <link href="~/dist/css/adminlte.min.css" rel="stylesheet" />
    <link href="~/plugins/overlayScrollbars/css/OverlayScrollbars.min.css" rel="stylesheet" />
    <link href="~/plugins/daterangepicker/daterangepicker.css" rel="stylesheet" />



    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    <script>
        var baseUrl = "@Url.Content("~")";
        var urlDatatableLanguage = baseUrl + "/DataTable/Internationalisation/es.txt";
    </script>
    <link href="~/css/Loader.css" rel="stylesheet" />
    @RenderSection("styles", required: false)
</head>


<body class="hold-transition sidebar-mini layout-fixed text-sm">
    <div class="loader" style="display:none">
        <div class="cssload-container">
            <div class="cssload-speeding-wheel"></div>
        </div>
    </div>


    <div class="wrapper">
        <!-- Navbar -->
        @*main-header navbar navbar-expand navbar-dark navbar-info text-sm*@
        <nav class="main-header navbar navbar-expand border-bottom-0 navbar-light navbar-white">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item">
                    @*44.25 * 30.98*@
                    <img src="~/Imagen/logo.jpg" width="190" height="30.98" />
                </li>
            </ul>
            <!-- SEARCH FORM -->
            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">

                <!-- Notifications Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" id="btn_salir" data-toggle="dropdown" href="#">
                        <i class="fas fa-sign-out-alt mr-2"></i> Salir
                    </a>

                </li>
                <li class="nav-item" style="display:none">
                    <a class="nav-link" data-widget="control-sidebar" data-slide="true" href="#">
                        <i class="fas fa-th-large"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container  main-sidebar elevation-4 sidebar-light-info-->

        <aside class="main-sidebar elevation-4 sidebar-dark-olive">
            <!-- Brand Logo -->
            <a href="@Url.Action("Index","Dashboard")" class="brand-link">
                <span class="brand-text font-weight-light"></span>

            </a>
            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/Imagen/avatar2.png" class="img-circle" alt="User Image">

                    </div>
                    <div class="info">

                        @Html.Raw(string.Format("<span class='d-block' id='usernameLogOn' style='color:white'><strong>{0}</strong></span>", @ViewData["nombreUsuario"]))
                        <a href="#"><i class="fa fa-circle text-success"></i> conectado</a>
                    </div>
                </div>
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column nav-child-indent nav-flat" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->

                        <li class="nav-item">
                            <a href="@Url.Action("Index","Dashboard")" class="nav-link active ">
                                <i class="nav-icon fas fa-home"></i>
                                <p>Inicio</p>
                            </a>
                        </li>



                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fa fa-list-alt"></i>
                                <p>
                                    Consultas
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_UnidadFlota).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"UnidadFlota")" class="nav-link ">
                                            <i class="fa fa-list"></i>
                                            <p>Lista de unidades de flota</p>
                                        </a>
                                    </li>
                                </ul>
                            }

                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_UnidadFlotaLima).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"UnidadFlotaLima",new {Id= "0"})" class="nav-link ">
                                            <i class="fa fa-list"></i>
                                            <p>Lista de unidades de flota Lima</p>
                                        </a>
                                    </li>
                                </ul>
                            }

                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_StockProducto).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"StockProducto",new {Id= "0"})" class="nav-link ">
                                            <i class="fa fa-list"></i>
                                            <p>Stock de Productos</p>
                                        </a>
                                    </li>
                                </ul>
                            }

                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_ConsultaEstadoActualFlota).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"EstadoActualFlota")" class="nav-link ">
                                            <i class="fa fa-list"></i>
                                            <p>Consulta Estado Flota PNP</p>
                                        </a>
                                    </li>
                                </ul>
                            }

                        </li>


                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fa fa-chart-line"></i>
                                <p>
                                    Reportes
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_ReporteHistorialUnidad).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"ReporteHistorialUnidad",new {Id= "0"})" class="nav-link ">
                                            <i class="fa fa-chart-bar"></i>
                                            <p>Historial de unidades</p>
                                        </a>
                                    </li>
                                </ul>
                            }


                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_AnexoIV).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"AnexoIV",new {Id= "0"})" class="nav-link ">
                                            <i class="fa fa-chart-bar"></i>
                                            <p>Anexo IV Reporte de Seguimiento</p>
                                        </a>
                                    </li>
                                </ul>
                            }


                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_AnexoV).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"AnexoV",new {Id= "0"})" class="nav-link ">
                                            <i class="fa fa-chart-bar"></i>
                                            <p>Anexo V Reporte de Costos</p>
                                        </a>
                                    </li>
                                </ul>
                            }

                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_ConsultaEstadoFlota).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"ConsultaEstadoFlota")" class="nav-link ">
                                            <i class="fa fa-list"></i>
                                            <p>Consulta Estado Flota ASBANC</p>
                                        </a>
                                    </li>
                                </ul>
                            }


                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_ConsultaFacturas).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"ConsultaFacturas")" class="nav-link ">
                                            <i class="fa fa-list"></i>
                                            <p>Consulta de Facturas</p>
                                        </a>
                                    </li>
                                </ul>
                            }

                        </li>


                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="fa fa-file"></i>
                                <p>
                                    Registro de Datos
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            @if (Model.Where(p => p.formc_vnombre_forms == Constantes.Op_Vehiculo).Any())
                            {
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="@Url.Action("Index" ,"Vehiculo",new {Id= "0"})" class="nav-link ">
                                            <i class="fa fa-car"></i>
                                            <p>Registro Datos del Vehículo</p>
                                        </a>
                                    </li>
                                </ul>
                            }
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("Index" ,"ActualizacionContraseña",new {Id= "0"})" class="nav-link ">
                                        <i class="fa fa-user-lock"></i>
                                        <p>Actualización de Contraseñas</p>
                                    </a>
                                </li>
                            </ul>


                        </li>
                        @*@if (WebSession.Opciones.Where(p => p.codigo == Constantes.Op_UnidadFlota).Any()){
                                <label style="margin-right: 4px;">' + obj.KmVidaUtilFabricante + '</label>
                                <a class="btn-Actualizar" data-KmVidaUtilFabricante="' + obj.KmVidaUtilFabricante + '" data-CodRepuesto="' + obj.CodRepuesto + '" data-Descripcion="' + obj.Descripcion + '" href="#"><i class="fas fa-edit"></i></a>'
                            }*@

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper text-sm">
            <!-- Content Header (Page header) -->
            @RenderBody()
            <!-- /.content-header -->
        </div>


        <!-- /.content-wrapper -->
        <footer class="main-footer text-sm">
            <strong>Copyright &copy; @DateTime.Now.Year .</strong>
            todos los derechos reservados.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 1.1
            </div>
        </footer>
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->



    <script src="~/plugins/jquery/jquery.min.js"></script>
    <script src="~/plugins/jquery-ui/jquery-ui.min.js"></script>
    <script>$.widget.bridge('uibutton', $.ui.button)</script>
    <script src="~/popper/popper.min.js"></script>
    <script src="~/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/plugins/moment/moment.min.js"></script>
    <script src="~/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <script src="~/dist/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <!-- AdminLTE for demo purposes -->
    <script src="~/dist/js/demo.js"></script>
    <script src="~/Scripts/sweetalert.min.js"></script>
    <script src="~/Scripts/validCampoFranz.js"></script>
    <script src="~/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Scripts/webApp.js"></script>
    <script src="~/js/Load.js"></script>
    <script src="~/ViewJs/Home.js"></script>
    <script src="~/js/CheckSession.js"></script>

    <script type="text/javascript">$(document).ready(function () {
            metodoValueNotEquals();
            load.init();
        });</script>
    @RenderSection("scripts", required: false)

</body>
</html>
